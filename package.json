{
  "name": "acme_reports_builder",
  "version": "1.0.0",
  "description": "This tool accepts xlsx and txt data files in a predefined format and generate insights in a csv file",
  "main": "index.js",
  "scripts": {
    "ingest": "node app.js ingest --filename=one.xlsx",
    "summary": "node app.js summary --category=Produce --year=2019 --month=4",
    "generate_report": "node app.js generate_report --filename=myOutputFile",
    "exit": "node app.js exit",
    "test": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanjeev-iitr-uk/acme_reports_builder.git"
  },
