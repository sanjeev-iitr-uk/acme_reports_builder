  },
  "homepage": "https://github.com/sanjeev-iitr-uk/acme_reports_builder#readme",
  "dependencies": {
    "chalk": "^3.0.0",
    "csv-stringify": "^5.3.6",
    "jest": "^25.1.0",
    "jest-html-reporter": "^2.8.0",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.26",
    "notarealdb": "^0.2.2",
    "xlsx": "^0.15.5",
    "yargs": "^12.0.2"
  }
